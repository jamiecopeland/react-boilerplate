{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "React Boilerplate",
  "main": "server/server.js",
  "scripts": {
    "build": "env UV_THREADPOOL_SIZE=100 NODE_ENV=production webpack                    --config webpack.production.js  --progress --colors --profile",
    "startHot": "env UV_THREADPOOL_SIZE=100 NODE_ENV=development webpack-dev-server     --config webpack.development.js --progress --colors --port 3000 --host 0.0.0.0 --content-base dist/ --inline --hot",
    "startRefresh": "env UV_THREADPOOL_SIZE=100 NODE_ENV=development webpack-dev-server --config webpack.development.js --progress --colors --port 3000 --host 0.0.0.0 --content-base dist/ --inline"
  },
  "author": "Jamie Copeland",
  "license": "MIT",
  "dependencies": {
    "react": "^0.13.3",
    "react-router": "^0.13.4"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "compression-webpack-plugin": "^0.2.0",
    "css-loader": "^0.19.0",
    "eslint": "^1.6.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.5.1",
    "file-loader": "^0.8.4",
    "node-sass": "^3.3.3",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "webpack-merge": "^0.2.0"
  }
}
